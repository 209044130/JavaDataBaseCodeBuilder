<AnchorPane minHeight="400.0" minWidth="600.0" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.codedb.controller.FrameMain">
    <children>
        <VBox alignment="CENTER" minHeight="400.0" minWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <SplitPane fx:id="topPane" maxHeight="-Infinity" orientation="VERTICAL" prefWidth="160.0" VBox.vgrow="ALWAYS">
                    <items>
                        <MenuBar fx:id="menuBar">
                            <menus>
                                <Menu mnemonicParsing="false" text="文件">
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="打开" />
                                    </items>
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="打开" />
                                    </items>
                                </Menu>
                                <Menu mnemonicParsing="false" text="编辑">
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="编辑" />
                                    </items>
                                </Menu>
                                <Menu mnemonicParsing="false" text="帮助">
                                    <items>
                                        <MenuItem mnemonicParsing="false" text="工具" />
                                    </items>
                                </Menu>
                            </menus>
                        </MenuBar>
                    </items>
                </SplitPane>
                <SplitPane dividerPositions="0.2, 0.8" VBox.vgrow="ALWAYS">
                    <items>
                        <SplitPane fx:id="leftPane" dividerPositions="0.5" minWidth="200.0" orientation="VERTICAL" prefHeight="200.0" prefWidth="200.0">
                            <items>
                                <TreeTableView fx:id="leftTreeTableView" minWidth="200.0" prefWidth="200.0" styleClass="leftTreeTableView" />
                                <ListView fx:id="leftToolListView" minWidth="200.0" prefWidth="200.0" styleClass="leftToolListView" />
                            </items>
                        </SplitPane>
                  <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0">
                    <tabs>
                      <Tab text="欢迎使用">
                           <content>
                              <VBox prefHeight="200.0" prefWidth="100.0">
                                 <children>
                                    <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="欢迎使用" textFill="#0083ff" VBox.vgrow="ALWAYS">
                                       <font>
                                          <Font name="Microsoft YaHei" size="34.0" />
                                       </font>
                                    </Label>
                                    <Label maxWidth="1.7976931348623157E308" text="介绍" />
                                 </children>
                              </VBox>
                           </content></Tab>
                    </tabs>
                  </TabPane>
                        <ListView fx:id="rightListView" minWidth="100.0" prefWidth="120.0" />
                    </items>
                </SplitPane>
                <VBox alignment="CENTER" maxHeight="35.0" minHeight="35.0" prefHeight="35.0" style="-fx-spacing: 0;" VBox.vgrow="ALWAYS">
                    <children>
                        <ProgressBar fx:id="bottomProcessBar" focusTraversable="false" maxHeight="15.0" maxWidth="1.7976931348623157E308" minHeight="15.0" prefHeight="15.0" progress="0.0">
                            <cursor>
                                <Cursor fx:constant="DEFAULT" />
                            </cursor>
                        </ProgressBar>
                        <TextField fx:id="status" editable="false" focusTraversable="false" maxHeight="20.0" minHeight="20.0" prefHeight="20.0" promptText="无" style="-fx-pref-width: 100%;" VBox.vgrow="ALWAYS">
                            <font>
                                <Font size="12.0" />
                            </font>
                            <cursor>
                                <Cursor fx:constant="DEFAULT" />
                            </cursor>
                        </TextField>
                    </children>
                </VBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
